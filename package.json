{
  "name": "getit-ui",
  "private": true,
  "description": "Components Design System for My Personal Projects",
  "author": {
    "name": "gitgitWi",
    "email": "wiii@kakao.com"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": ">=16"
  },
  "workspaces": {
    "packages": [
      "atoms/**/*",
      "molecules/**/*",
      "organisms/**/*",
      "stories"
    ]
  },
  "scripts": {
    "stories": "yarn workspace @getit-ui/stories",
    "components": "yarn workspaces foreach -ptiv --exclude getit-ui --exclude @getit-ui/stories",
    "button": "yarn workspace @getit-ui/button",
    "text": "yarn workspace @getit-ui/text",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-storysource": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "core-js": "^3.21.1",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "latest",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "storybook-addon-turbo-build": "^1.0.1",
    "storybook-mobile": "^1.0.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.5",
    "webpack": "^5.76.0"
  },
  "resolutions": {
    "@storybook/react/webpack": "^5"
  }
}
